    "dotenv": "^16.4.5",
    "expo": "^51.0.38",
    "expo-constants": "~16.0.1",
    "expo-font": "~12.0.5",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.14",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
